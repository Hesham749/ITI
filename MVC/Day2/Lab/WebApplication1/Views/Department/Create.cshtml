@model Department
@{
	ViewData["Title"] = "Create";
}

<form method="post">
	<label asp-for="Id">ID<input asp-for="Id" class="form-control" /></label>
	<br /><span asp-validation-for="Id"></span>
	<label asp-for="Name">Name<input asp-for="Name" class="form-control" /></label>
	<br /><span asp-validation-for="Name"></span>
	<label asp-for="Capacity">Capacity<input asp-for="Capacity" class="form-control" /></label>
	<br /><span asp-validation-for="Capacity"></span>
	<br /> <input type="submit" value="Create" class="btn btn-success" />
</form>




@section Scripts {

	<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}